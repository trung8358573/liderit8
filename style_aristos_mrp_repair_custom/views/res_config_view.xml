<?xml version="1.0" encoding="utf-8"?>
<openerp>

    <data>
        <record id="view_mrp_config_journal" model="ir.ui.view">
            <field name="name">mrp journal settings</field>
            <field name="model">mrp.config.settings</field>
            <field name="inherit_id" ref="mrp.view_mrp_config"/>
            <field name="arch" type="xml">
                <xpath expr="//label[@string='Products']" position="before">
                    <label for="id" string="Reparaciones"/>
                     <div>
                        <div>
                            <label for="repair_journal_id" />
                            <field name="repair_journal_id" class="oe_inline"/>
                        </div>
                        <div>
                            <label for="repair_location_dest_id" />
                            <field name="repair_location_dest_id" class="oe_inline"/>
                        </div>
                    </div>

                </xpath>
               
            </field>
        </record>


    </data>
</openerp>